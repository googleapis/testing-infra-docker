{
  "extends": [
    "config:base"
  ],
  "ignorePaths": [
    "go/*/Dockerfile"
  ],
  "enabledManagers": ["regex"],
  "regexManagers": [
    {
      "fileMatch": ["^java/cloudbuild.yaml", "^java/cloudbuild-test.yaml"],
      "matchStrings": ["  _GRAALVM_VERSION: '(?<currentValue>.*?)'"],
      "depNameTemplate": "ghcr.io/graalvm/jdk",
      "datasourceTemplate": "docker"
    }
  ]
}
