{
  "extends": [
    "config:base"
  ],
  "ignorePaths": [
    "go/*/Dockerfile"
  ],
  "enabledManagers": ["regex"],
  "regexManagers": [
    {
      "fileMatch": ["^java/cloudbuild.yaml$", "^java/cloudbuild-test.yaml$"],
      "matchStrings": ["substitutions:\n  _GRAALVM_VERSION: '(?<currentValue>.*?)'"],
      "depNameTemplate": "jdk",
      "datasourceTemplate": "docker",
      "packageNameTemplate": "ghcr.io/graalvm/jdk"
    }
  ]
}
